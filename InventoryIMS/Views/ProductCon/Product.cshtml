@{
    ViewData["Title"] = "Dashboard";
}
@model IEnumerable<InventoryIMS.Models.TblProduct>


<h4 class="alert-heading align-content-md-center"> Mobile Accessories </h4>


@*<button asp-action="Create" asp-controller="Login" class=" btn btn-primary">LOGIN</button>
*@
<input type='button' value='Login' class="btn btn-labeled btn-success" id='myButton' onClick='redirectOnClick()' />
<script>
    function redirectOnClick() {
        document.location = 'Logincon/Login';
    }</script>
<br />

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Product_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product_Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Product_Qty)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Product_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Product_Price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Product_Qty)
                </td>
                <td>
                </td>
            </tr>
        }
    </tbody>
</table>

